{
  "cli": {
    "fmtVersion": 3
  },
  "name": "Build Helper",
  "description": "Augmented Reality tools for Dual Universe builders",
  "sourcePath": "src",
  "outputPath": "out",
  "libs": [],
  "builds": {
    "main": {
      "name": "main",
      "type": "control",
      "slots": {}
    }
  },
  "targets": {
    "development": {
      "name": "development",
      "minify": false,
      "handleErrors": false,
      "variables": {
        "env": "development"
      }
    },
    "production": {
      "name": "production",
      "minify": true,
      "handleErrors": false,
      "variables": {
        "env": "production"
      }
    }
  },
  "internalPaths": [
    "autoconf/",
    "cpml/",
    "pl/",
    "utils/event"
  ]
}